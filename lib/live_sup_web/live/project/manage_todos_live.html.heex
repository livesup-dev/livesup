<%= LiveSupWeb.Project.Components.ManageTodosHeaderComponent.render(%{project: @project}) %>

<div class="flex flex-col gap-8">
    <ul class="list-disc list-inside text-slate-900 dark:text-slate-200">
        <%= for(todo <- @project.todos) do %>
            <li><%= todo.title %></li>
        <% end %>
    </ul>
</div>
